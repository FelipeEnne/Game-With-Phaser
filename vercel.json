{
  "version": 2,
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node"
    },
    {
        "src": "dist/**",
        "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
        "src": "/",
        "dest": "public/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "dist/$1"
    }
  ]
}