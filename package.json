{
  "name": "game_with_phaser",
  "version": "1.0.0",
  "description": "A game Mario-style",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "jest-watch": "jest --watch",
    "install-web": "npm install --force --save-dev webpack",
    "build": "webpack",
    "watch": "webpack --watch",
    "npx-fix": "npx eslint src/  --fix",
    "start": "node server.js"
  },
  "author": "Felipe Enne",
  "license": "MIT",
  "private": true,
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/test/mocks/styleMock.js",
      "\\.(gif|ttf|eot|svg|png)$": "<rootDir>/test/mocks/fileMock.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/runtime": "^7.9.2",
    "@types/babel-core": "^6.25.6",
    "babel-core": "^6.26.3",
    "babel-jest": "^25.2.6",
    "babel-loader": "^9.1.2",
    "babel-plugin-transform-async-to-generator": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.6.0",
    "eslint": "^8.9.0",
    "eslint-plugin-import": "^2.20.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.0.4",
    "jest": "^27.5.1",
    "jest-canvas-mock": "^2.2.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^1.2.1",
    "webpack": "^5.75.0",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "express": "^4.17.1",
    "node-fetch": "^2.6.0",
    "phaser": "^3.0.0",
    "phaser3-rex-plugins": "^1.1.11"
  },
  "engines": {
    "node": "18.x"
  },
  "repository": "https://github.com/FelipeEnne/Game-With-Phaser.git"
}
