{
  "name": "game_with_phaser",
  "version": "1.0.0",
  "description": "A game Mario-style",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "jest-watch": "jest --watch",
    "install-web": "npm install --force --save-dev webpack",
    "build": "webpack",
    "watch": "webpack --watch",
    "npx-fix": "npx eslint src/  --fix",
    "start": "node api/server.js"
  },
  "author": "Felipe Enne",
  "license": "MIT",
  "private": true,
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/test/mocks/styleMock.js",
      "\\.(gif|ttf|eot|svg|png)$": "<rootDir>/test/mocks/fileMock.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.5",
    "@babel/runtime": "^7.21.5",
    "@types/babel-core": "^6.25.7",
    "babel-core": "^4.7.16",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.40.0",
    "eslint-plugin-import": "^2.27.5",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.1",
    "jest": "^29.5.0",
    "jest-canvas-mock": "^2.5.0",
    "raw-loader": "^4.0.2",
    "style-loader": "^3.3.2",
    "webpack": "^5.82.1",
    "webpack-dev-server": "^4.15.0"
  },
  "dependencies": {
    "@vercel/node": "^2.14.2",
    "express": "^4.18.2",
    "node-fetch": "^3.3.1",
    "phaser": "^3.60.0",
    "phaser3-rex-plugins": "^1.60.1"
  },
  "engines": {
    "node": "18.x"
  },
  "repository": "https://github.com/FelipeEnne/Game-With-Phaser.git"
}
